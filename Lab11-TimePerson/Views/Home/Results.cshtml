@model List<Lab11_TimePerson.Models.TimePerson>
<link rel="stylesheet" href="~/Styles/StyleSheet.css" />


<!--Traverse through the Model and output each of the line items in the csv file-->
<!--Make 9 columns for: NAME    YEAR    Accomplishment  etc.  Can put results in a table.-->

<h1>Time Magazine Person of the Year Search Results</h1>
<div>
    <table>
        <tr>
            <th>Year</th>
            <th>Honor</th>
            <th>Name</th>
            <th>Country</th>
            <th>BirthYear</th>
            <th>DeathYear</th>
            <th>Title</th>
            <th>Category</th>
            <th>Context</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Year</td>
                <td>@item.Honor</td>
                <td>@item.Name</td>
                <td>@item.Country</td>
                <td>@item.BirthYear</td>
                <td>@item.DeathYear</td>
                <td>@item.Title</td>
                <td>@item.Category</td>
                <td>@item.Context</td>
            </tr>
        }

    </table>
</div>